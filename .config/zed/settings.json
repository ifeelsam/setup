{
  "tab_bar": {
    "show": true
  },
  "show_inline_completions": false,
  "assistant": {
    "default_model": {
      "provider": "ollama",
      "model": "llama3.1:latest"
    },
    "version": "2"
  },
  "ui_font_size": 15,
  "buffer_font_size": 14,
  "vim_mode": true,
  "theme": {
    "mode": "system",
    "light": "Ayu Light",
    "dark": "Ayu Dark"
  },
  "buffer_font_family": "FiraCode Nerd Font Mono",
  "experimental.theme_overrides": {
    "background.appearance": "blurred",
    "background": "#D9D9D901",
    // "background": "#D9FFFF01",
    "editor.background": "#00000070",
    "editor.gutter.background": "#00000070",
    "tab_bar.background": "#00000030",
    "terminal.background": "#00000030",
    "toolbar.background": "#00000030",
    "tab.active_background": "#28282820",
    "tab.inactive_background": "#00000000",
    "status_bar.background": "#00000030",
    "title_bar.background": "#00000030",
    "panel.background": "#01000060",
    "border.variant": "#00000000",
    "scrollbar.track.border": "#00000000",
    "editor.active_line.background": "#00000000"
  }
}
